#!/bin/bash

IMAGE_NAME="mobula/postgresql"
#GITREPO="https://github.com/mobula/docker-postgresql-postgis"
GITREPO="https://github.com/mobula/dokku-pg-dockerfiles"
TMPDIR="/tmp/dokku-pg-dockerfiles"

# Build docker image
git clone $GITREPO $TMPDIR
docker build -q=true -t $IMAGE_NAME $TMPDIR
rm -rf $TMPDIR
